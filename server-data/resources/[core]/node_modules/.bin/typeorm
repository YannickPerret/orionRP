#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tchoune/Documents/dev/fiveM/orionRP/server-data/resources/[core]/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.1_mysql2@3.11.3/node_modules/typeorm/node_modules:/Users/tchoune/Documents/dev/fiveM/orionRP/server-data/resources/[core]/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.1_mysql2@3.11.3/node_modules:/Users/tchoune/Documents/dev/fiveM/orionRP/server-data/resources/[core]/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tchoune/Documents/dev/fiveM/orionRP/server-data/resources/[core]/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.1_mysql2@3.11.3/node_modules/typeorm/node_modules:/Users/tchoune/Documents/dev/fiveM/orionRP/server-data/resources/[core]/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.4.1_mysql2@3.11.3/node_modules:/Users/tchoune/Documents/dev/fiveM/orionRP/server-data/resources/[core]/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
